Node* deleteNode(Node* head, int n){
  if(!head) 
    return head;
  Node* slow = head;
  Node* fast = head;

  for(int i=1;i<=n;i++){
    fast = fast->next;
  }

  if(!fast)
    return head->next;

  while(fast){
    slow = slow->next;
    fast = fast->next;
  }

  Node* toDelete = slow->next;
  slow->next = toDelete->next;
  delete toDelete;

  return head;
}
