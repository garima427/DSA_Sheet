ListNode* addTwoNumbers(ListNode* l1, ListNode* l2) {
        ListNode* dummyHead = new ListNode(-1);
        ListNode* tail = dummyHead;
        int carry = 0, sum;
        while(l1 || l2){
            sum = carry;
            if(l1) sum+=l1->val;
            if(l2) sum+=l2->val;
            
            carry = sum/10;
            ListNode* newNode = new ListNode(sum%10);
            tail->next = newNode;
            tail = newNode;

            if(l1) l1= l1->next;
            if(l2) l2 = l2->next;
        }
       
        if(carry){
            tail->next = new ListNode(1);
        }
        return dummyHead->next;
    }
