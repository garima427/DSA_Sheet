bool dfsCheck(int i, vector<vector<int>> &adj,vector<int> &vis,vector<int> &path){
  vis[i]=1;
  path[i]=1;

  for(auto adjNode : adj[i]){
    if(!vis[adjNode]){
      if(dfsCheck(adjNode,adj,vis,path))
          return true;
      }
    else if(path[adjNode]){
      return true;
    }
  }
  path[i]=0;
  return false;
}
bool isCycle(int n, vector<vector<int>> &adj){
  vector<int> vis(n+1,0);
  vector<int> path(n+1,0);
  for(int i=0; i<=n; i++){
    if(!vis[i]){
      if(dfsCheck(i,adj,vis,path))
          return true;
    }
  }
  return false;
}
