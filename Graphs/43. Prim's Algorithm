int minSpanningTree(vector<vector<vector<int>>> edges,int v){
  int sum = 0;
  vector<int> vis(v,0);
  priority_queue<pair<int, int>,
	vector<pair<int, int> >, greater<pair<int, int>>> pq;

  pq.push({0,0});//wt,node
  while(!pq.empty()){
    int wt = pq.top().first;
    int node = pq.top().second;
    pq.pop();
    
    if(vis[node]) continue;
  
    sum+=wt;
    vis[node] = 1;
    
    for(auto it : edges[node]){
      int adjNode = it[0];
      int edW = it[1];
      if(!vis[adjNode]){
        pq.push({edW,adjNode});
      }
    }

    }
    return sum;
  }

