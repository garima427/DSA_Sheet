bool detect(int node,int parent,vector<vector<int>> &adj,vector<int> &vis){
  vis[node]=1;

    for(int adjNode:adj[node]){
      if(!vis[adjNode]){
       if(detect(adjNode,node,adj,vis))
          return true;
      }
      else{
        if(parent!=adjNode){
          return true;
        }
      }
    }
  return false;
}
bool isCycle(int n,vector<vector<int>> &adj){
  vector<int> vis(n+1,0);
  for(int i=1; i<=n; i++){
    if(!vis[i]){
      if(detect(i,-1,adj,vis)) 
          return true;
    }
  }
  return false;
}
